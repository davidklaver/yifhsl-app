<div class="loginContainer">
	<h1>Please Log in below:</h1>
	<%= form_tag "/login", method: :post do %>
	  <div>
	    Username: <input type="username" name="username">
	  </div>
	  <div>
	    Password: <input type="password" name="password">
	  </div>
	  <input type="submit" value="Log In">
	<% end %>
</div>
